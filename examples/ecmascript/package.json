{
  "name": "jest-cucumber-examples",
  "version": "1.0.0",
  "description": "Execute Gherkin scenarios in Jest",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --color",
    "jest": "jest --verbose"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^23.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/*.steps.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
